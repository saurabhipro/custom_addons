<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <record id="helpdesk_ticket_seq" model="ir.sequence">
        <field name="name">Helpdesk Ticket Sequence</field>
        <field name="code">sh.helpdesk.ticket</field>
        <field name="prefix">TICKET/</field>
        <field name="padding">4</field>
        <field name="company_id" eval="False" />
        <field name="implementation">standard</field>
    </record>

    <record id="helpdesk_tikcet_dashboard" model="ticket.dashboard">
        <field name="name">Ticket Dashboard</field>
        <field name="active" eval="True"/>
    </record>
</odoo>
