<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <template id="property_details_template" name="Property Template">
            
        <div class="container mt-4">
            <div class="text-center" style="text-align:center;padding:4rem">
                <h1 style="font-family: sans-serif;">Welcome to UDAN</h1>
                <p>Chhatrapati SambhajiNagar</p>
            </div>

            <div class="udan-emergency mt-5 p-3" style="background:lightgrey;padding:4rem">
                <h3 style="font-family: sans-serif;">Instant Access to Emergency Services</h3>
                <a href="tel:112" class="btn btn-danger">Call Now</a>
            </div>

            <t t-foreach="property" t-as="pr"></t>
            <div class="udan-details mt-4" style="padding:4rem;">
                <h3 style="font-family: sans-serif;"><strong>Property Identification Number:</strong> <span></span></h3>
                <p><strong>Digital Door Number:</strong> F-7/7/C/0001</p>
                <p><strong>Latitude / Longitude:</strong> <span t-esc="pr.latitude"></span> / <span t-esc="pr.longitude"></span></p>
                <p><strong>Block:</strong> 7/C</p>
                <p><strong>Address:</strong> S.No. 20/1, Vedkar Corner, Hindu Rashtra Chowk, Garkheda Parisar</p>
            </div>

            <div class="udan-gallery mt-4">
            </div>
        </div>

</template>
</odoo>